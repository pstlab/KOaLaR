[impairments:
	(?x rdf:type sir:Measurement), 
	(?x sir:measures ?y), 
	(?x dul:hasConstituent ?w), 
	(?x sir:measurementOutcome ?z), 
	(?z sir:hasICFScore ?v), 
	greaterThan(?v, 0), lessThan(?v, 5), 
	makeTemp(?i)
	->
	(?i rdf:type sir:CognitiveImpairment),
	(?i sir:concernsPatient ?w), 
	(?i sir:concernsQuality ?y), 
	(?i dul:satisfies ?x)]


[opportunities:
	(?i rdf:type sir:CognitiveImpairment), 
	(?i sir:concernsQuality ?q), 
	(?s rdf:type sir:Stimulus),
	(?s dul:hasConstituent ?f),
	(?s sir:hasEffectOn ?q),
	makeTemp(?x),
	->
	(?x rdf:type sir:StimulationOpportunity)
	(?x dul:classifies ?i), 
	(?x dul:classifies ?f),
	(?x dul:classifies ?s),
	(?s sir:canAfford ?i)]



 


	