[high-heartrate:
	(?o rdf:type ssn:Observation), (?o ssn:featureOfInterest ?f),
	(?e koala:hasObservableFeature ?f), (?o ssn:hasOutput ?d), (?d ssn:hasValue ?v),
	(?v koala:hasHeartRateValue ?l), (koala:health_high_heart_frequency koala:hasHeartRateValue ?max),
	greaterThan(?l, ?max), makeSkolem(?event, ?o),
	makeTemp(?i), now(?t)
	->
	print('[Event] High heart rate on feature ', ?l, ?e, ?event),
	(?event rdf:type koala:HighHeartFrequency), (?event koala:concerns ?f), (?event ssn:isProducedBy ?o), 
	(?i rdf:type koala:TimeInstant), (?event ssn:observationRuleTime ?i), (?i koala:hasDate ?t)]


[low-heartrate:
	(?o rdf:type ssn:Observation), (?o ssn:featureOfInterest ?f),
	(?e koala:hasObservableFeature ?f), (?o ssn:hasOutput ?d), (?d ssn:hasValue ?v),
	(?v koala:hasHeartRateValue ?l), (koala:health_low_heart_frequency koala:hasHeartRateValue ?min),
	lessThan(?l, ?max), makeSkolem(?event, ?o),
	makeTemp(?i), now(?t)
	->
	print('[Event] Low heart rate on feature ', ?l, ?e, ?event),
	(?event rdf:type koala:LowHeartFrequency), (?event koala:concerns ?f), (?event ssn:isProducedBy ?o), 
	(?i rdf:type koala:TimeInstant), (?event ssn:observationRuleTime ?i), (?i koala:hasDate ?t)]
	
	
[pain-attack:
	(?e1 rdf:type koala:HighBloodPressure), (?e1 koala:concerns ?f1), (?p koala:hasObservableFeature ?f1), 
	(?e2 rdf:type koala:HighHeartRate), (?e2 koala:concerns ?f2), (?p koala:hasObservableFeature ?f2),
	(?p rdf:type koala:Patient),
	makeSkolem(?event, ?e1, ?e2), makeTemp(?i), now(?t)
	->
	print('[Event] Pain attack from events ', ?e1, ?e2),
	(?event rdf:type koala:PainAttack), (?event koala:concerns ?f1), (?event koala:concerns ?f2),  
	(?i rdf:type koala:TimeInstant), (?event ssn:observationRuleTime ?i), (?i koala:hasDate ?t)]


	
// analyze observations concerning interactions with the robot 

[voice-command:
	(?o rdf:type ssn:Observation), (?o ssn:featureOfInterest ?f),
	(?e koala:hasObservableFeature ?f), (?o ssn:hasOutput ?d), (?d ssn:hasValue ?v),
	(?v koala:hasVoiceCommandValue ?l), 
	makeSkolem(?event, ?o), makeTemp(?i), now(?t)
	->
	print('[Event] Voice command on feature ', ?l, ?e, ?event),
	(?event rdf:type koala:IssuingVoiceCommand), (?event koala:concerns ?f), (?event ssn:isProducedBy ?o), 
	(?i rdf:type koala:TimeInstant), (?event ssn:observationRuleTime ?i), (?i koala:hasDate ?t)]




// combine events to detect activities

[cooking:
	(?e1 rdf:type koala:MotionEvent), (?e1 koala:concerns ?f1), (?k koala:hasObservableFeature ?f1), 
	(?e2 rdf:type koala:HighLuminosity), (?e2 koala:concerns ?f2), (?k koala:hasObservableFeature ?f2),
	(?k rdf:type koala:Kitchen),
	makeSkolem(?activity, ?e1, ?e2), makeTemp(?i), now(?t)
	->
	print('[Activity] Cooking from events ', ?e1, ?e2),
	(?activity rdf:type koala:Cooking), (?activity koala:concerns ?f1), (?activity koala:concerns ?f2),  
	(?i rdf:type koala:TimeInstant), (?activity ssn:observationRuleTime ?i), (?i koala:hasDate ?t)]
	






 


	